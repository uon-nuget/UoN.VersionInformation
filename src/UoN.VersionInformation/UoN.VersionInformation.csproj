<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.5</TargetFramework>
    <PackageVersion>1.0.0-beta1</PackageVersion>

    <Authors>UoN Application Development</Authors>

    <Description>
      .NET Standard solution for loading data from an arbitrary source (via an implemented provider) and returning it as a .NET Object.
    </Description>

    <PackageReleaseNotes>
      First release.
    </PackageReleaseNotes>

    <Copyright>Copyright 2018 (c) University of Nottingham.</Copyright>

    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://raw.githubusercontent.com/uon-nuget/UoN.VersionInformation/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/uon-nuget/UoN.VersionInformation</PackageProjectUrl>
    <RepositoryUrl>https://github.com/uon-nuget/UoN.VersionInformation</RepositoryUrl>
    <PackageTags>uon version netstandard</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

</Project>
